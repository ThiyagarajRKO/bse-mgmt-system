        //if (peeled_product_id && peeled_product_id != "null") {
        url = "/api/v1/peeled/dispatch";
        method = "PUT"

        input_params = {
          peeled_dispatch_id,
          peeled_dispatch_data: {
            ...input_params,
          },
        };
      }

      $.ajax({
        url,
        method,
        dataType: "json",
        contentType: "application/json",
        data: JSON.stringify(input_params),
        crossDomain: true,
        processData: false,
        success: (data) => {
