  <script type="text/javascript">
    jQuery(document).ready(function ($) {
      // Declarations
      toastr.options = {
        closeButton: false,
        debug: false,
        newestOnTop: true,
        progressBar: true,
        positionClass: "toast-top-right",
        preventDuplicates: false,
        onclick: null,
        showDuration: "300",
        hideDuration: "1000",
        timeOut: "5000",
        extendedTimeOut: "1000",
        showEasing: "swing",
        hideEasing: "linear",
        showMethod: "fadeIn",
        hideMethod: "fadeOut",
      };

      changePercentage("dispatchCircle", "dispatchPercentage", null, 0);
      changePercentage(
        "preprocessingCircle",
        "preprocessingPercentage",
        "preprocessingHR",
        0
      );
      changePercentage(
        "preprocessingDispatchCircle",
        "preprocessingDispatchPercentage",
        "preprocessingDispatchHR",
        0
      );
      changePercentage("packingCircle", "packingPercentage", "packingHR", 0);

      function changePercentage(circle_id, percentage_id, hr_id, percent) {
        $(`#${circle_id}`).attr("data-percentage", percent);
        $(`#${percentage_id}`).text(`${percent}%`);
        // Get radius of the svg's circle.complete
        radius = $(`#${circle_id}`).find($("circle.complete")).attr("r");
        // Get circumference (2Ï€r)
        circumference = 2 * Math.PI * radius;
        // Get stroke-dashoffset value based on the percentage of the circumference
        strokeDashOffset = circumference - (percent * circumference) / 100;
        // Transition progress for 1.25 seconds
        $(`#${circle_id}`)
          .find($("circle.complete"))
          .animate({ "stroke-dashoffset": strokeDashOffset }, 1250);

        if (percent > 0) {
          $(`#${hr_id}`).css({ opacity: "0.8" });
        } else {
          $(`#${hr_id}`).css({ opacity: "0.2" });
        }
      }
    });
  </script>
