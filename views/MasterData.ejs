<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>BSE</title>

  <style type="text/css">
    @font-face {
      font-family: "CassandraPersonalUse";
      src: url("/public/fonts/CassandraPersonalUse/CassandraPersonalUse-Regular.ttf?v=2") format("truetype");
    }

    @font-face {
      font-family: "CerebriSansPro-Regular";
      src: url("/public/fonts/CerebriSansPro/CerebriSansPro-Regular.ttf?v=2") format("truetype");
    }

    body {
      font-family: "CerebriSansPro-Regular";
      background-color: #f7f9fd;
    }

    .topMenu {
      display: block;
      color: rgb(44 44 44);
      text-align: center;
      padding: 0px;
      text-decoration: none;
      border-right: 0px solid #1F2D3E;
      padding-bottom: 3px !important;
      width: 25vh !important;
      cursor: pointer;
    }

    #lastMenu {
      border-right: 0px solid #414a57;
    }

    .topMenu:hover {
      /* border-bottom: 1px solid #1F2D3E; */
      color: #EA5453;
      /* font-weight: bold; */
    }

    .menuActiveStatus {
      color: #f7f9fd;
      display: block !important;
    }

    .menuActiveStatus.active {
      color: #EA5453;
      display: block !important;
    }
  </style>
</head>

<body>
  <div class="d-flex justify-content-center">
    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center mx-3">
      <div class=" fs-3">
        <i class="fa-solid fa-warehouse"></i>
      </div>
      <div class="pt-2">
        <span>Units</span>
      </div>


    </div>

    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center mx-3">
      <div class=" fs-3">
        <i class="fa-solid fa-users"></i>
      </div>
      <div class="pt-2">
        <span>Vendors</span>
      </div>
    </div>

    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center mx-3">
      <div class=" fs-3">
        <i class="fa-solid fa-boxes-packing"></i>
      </div>
      <div class="pt-2">
        <span>Packages</span>
      </div>
    </div>

    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center mx-3">
      <div class=" fs-3">
        <i class="fa-solid fa-shrimp"></i>
      </div>
      <div class="pt-2">
        <span>Products</span>
      </div>
    </div>

    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center mx-3">
      <div class=" fs-3">
        <i class="fa-solid fa-truck"></i>
      </div>
      <div class="pt-2">
        <span>Vehicles</span>
      </div>
    </div>

    <div class="topMenu col-md-2 col-lg-2 d-flex flex-column justify-content-center align-items-center">
      <div class=" fs-3">
        <i class="fa-regular fa-id-card"></i>
      </div>
      <div class="pt-2">
        <span>Drivers</span>
      </div>
    </div>

  </div>
</body>

<script type="text/javascript">
  jQuery(document).ready(function ($) {

    toastr.options = {
      closeButton: false,
      debug: false,
      newestOnTop: true,
      progressBar: true,
      positionClass: "toast-top-right",
      preventDuplicates: false,
      onclick: null,
      showDuration: "300",
      hideDuration: "1000",
      timeOut: "5000",
      extendedTimeOut: "1000",
      showEasing: "swing",
      hideEasing: "linear",
      showMethod: "fadeIn",
      hideMethod: "fadeOut",
    };


  });
</script>

