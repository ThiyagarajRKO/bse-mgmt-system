<!-- @format -->

<!DOCTYPE html>
<html lang="en">
  <!-- title logo -->
  <link rel="icon" href="Images/voter.png" type="image/icon type" />
  <link
    rel="stylesheet"
    href="Plugins/bootstrap-4.3.1-dist/css/bootstrap.min.css"
  />
  <script src="Plugins/bootstrap-4.3.1-dist/js/bootstrap.min.js"></script>
  <script src="Plugins/jquery/jquery-3.5.1.min.js"></script>
  <link
    rel="stylesheet"
    type="text/css"
    href="Plugins/DataTables/datatables.min.css"
  />

  <!-- notification jquery -->
  <link rel="stylesheet" href="Plugins/Toastr/toastr.min.css" />
  <script src="Plugins/Toastr/toastr.min.js"></script>

  <script
    type="text/javascript"
    src="Plugins/DataTables/datatables.min.js"
  ></script>
  <link rel="stylesheet" href="css/style.css" />

  <!-- Font Awesome -->
  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
  />

  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Citizen Login</title>
    <style type="text/css"></style>
  </head>

  <body>
    <div
      class="navbar navbar-expand-lg navbar-dark"
      style="background-color: #771daa"
    >
      <div
        class="col-md-12"
        style="
          display: flex;
          align-items: baseline;
          justify-content: space-between;
        "
      >
        <div>
          <img class="Vlogo" src="Images/voter.png" alt="" />
        </div>
        <div>
          <h6 class="Vheading">E-Voting Management System</h6>
        </div>
        <div>
          <h6 class="logout">logout</h6>
        </div>
      </div>
    </div>
    <div class="container" style="display: flex; justify-content: center">
      <div class="col-xl-9 col-md-12 col-sm-12 p-4 panel3">
        <div id="voting-box" class="col-md-12">
          <h3 class="text-center headL my-2">Voting</h3>

          <table class="table table-hover" id="resultTable">
            <thead>
              <tr>
                <th scope="col">Party Symbol</th>
                <th scope="col">Parties Name</th>
                <th scope="col">count</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>

          <div
            class="form-group"
            style="margin-top: 20px; display: flex; justify-content: center"
          >
            <input
              class="btn-outline-custom"
              id="download"
              type="button"
              name="download"
              value="download"
            />
          </div>
        </div>
      </div>
    </div>
  </body>

  <!-- App js -->
  <script src="custom/App.js"></script>

  <script type="text/javascript">
    jQuery(document).ready(function ($) {
      toastr.options = {
        closeButton: false,
        debug: false,
        newestOnTop: true,
        progressBar: true,
        positionClass: "toast-top-right",
        preventDuplicates: false,
        onclick: null,
        showDuration: "300",
        hideDuration: "1000",
        timeOut: "5000",
        extendedTimeOut: "1000",
        showEasing: "swing",
        hideEasing: "linear",
        showMethod: "fadeIn",
        hideMethod: "fadeOut",
      };
      App.checkToken();

      $("#resultTable").dataTable({
        serverSide: true,

        ajax: {
          url: "",
          type: "POST",

          headers: {
            Authorization: App.getToken(),
          },
        },
        columns: [{ data: null }, { data: "" }, { data: null }],
        columnDefs: [
          {
            targets: [0],
            render: function (data, type, row) {
              return ()


            },
          },
          {
            targets: [2],
            render: function (data, type, row) {
              return ()


            },
          },
        ],

      });

      $(".logout").on("click", () => {
        App.logout();
      });


    });
  </script>
</html>
